#!/usr/bin/env python
from fts3.cli import Banning
import logging
import sys
import traceback


try:
    banning = Banning(sys.argv[1:])
    banning()
except Exception, e:
    logging.critical(str(e))
    if logging.getLogger().getEffectiveLevel() == logging.DEBUG:
        traceback.print_exc()
    sys.exit(1)
